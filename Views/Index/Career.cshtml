@model IEnumerable<StarSecurityServices.Models.Vacancies>
@{
    ViewData["Title"] = "Career";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="page-title-area page-title-style-two">
<div class="container">
<div class="page-title-content">
<h2>Career With Us</h2>
<ul>
<li>
<a  href="@Url.Action("Index", "Index")">
<i class="bx bx-home"></i>
Home
</a>
</li>
<li class="active">Career</li>
</ul>
</div>
</div>
</div>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Company Name</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
@foreach(var item in Model){
    <tr>
      <th scope="row"></th>
      < a href="#"><td>@Html.DisplayFor(modelitem => item.EmpName)</td>< /a >
      <td>@Html.DisplayFor(modelitem => item.EmpAdd)</td>
      <td>@Html.DisplayFor(modelitem => item.EmpCon)</td>
      <td>@Html.DisplayFor(modelitem => item.EmpEdu)</td>
      <td>@Html.DisplayFor(modelitem => item.Depart)</td>
      <td>@Html.DisplayFor(modelitem => item.Role)</td>
      <td>@Html.DisplayFor(modelitem => item.Grade)</td>
    </tr>
}
  </tbody>
</table>




