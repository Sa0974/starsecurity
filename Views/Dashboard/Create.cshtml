@model StarSecurityServices.Models.Vacancies
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/Dashboard.cshtml";
}

<div class="container ">
<div class="row">
    <div class="col-lg-12 col-sm-12 mt-5">
        <form asp-action="Create"  enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EmpName" ></label>
                <input asp-for="EmpName" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="EmpName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmpAdd"></label>
                <input asp-for="EmpAdd" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="EmpAdd" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmpCon"></label>
                <input asp-for="EmpCon" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="EmpCon" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmpEdu"></label>
                <input asp-for="EmpEdu" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="EmpEdu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Depart"></label>
                <input asp-for="Depart" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="Depart" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Role"></label>
                <input asp-for="Role" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="Role" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Grade"></label>
                <input asp-for="Grade" class="form-control form-control form-control-lg form-control-a" />
                <span asp-validation-for="Grade" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>

